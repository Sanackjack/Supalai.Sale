{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Caching": {
    "Distributed": {
      "Provider": "Redis",
      "Redis": {
        "Configuration": "192.168.2.101:6379",
